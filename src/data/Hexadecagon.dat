	BCH = f*(BB+BD)/4-g*(FJ+FL)/2+e*BC/8;
	BDH = f*(BC+BE)/4-g*(FC+FL)/2+e*BD/8;
	BEH = f*(BD+BF)/4-g*(FB+FC)/2+e*BE/8;
	BFH = f*(BA+BE)/4-g*(FB+FH)/2+e*BF/8;

	CAF = f*v*(CA+CF)/2-g*v*FD+(1-e*v)*CA;
	CAB = f*v*(CB+CA)/2-g*v*FE+(1-e*v)*CA;
	CBA = f*v*(CB+CA)/2-g*v*FE+(1-e*v)*CB;
	CBC = f*v*(CB+CC)/2-g*v*FK+(1-e*v)*CB;
	CCB = f*v*(CB+CC)/2-g*v*FK+(1-e*v)*CC;
	CCD = f*v*(CC+CD)/2-g*v*FL+(1-e*v)*CC;
	CDC = f*v*(CC+CD)/2-g*v*FL+(1-e*v)*CD;
	CDE = f*v*(CE+CD)/2-g*v*FC+(1-e*v)*CD;
	CED = f*v*(CE+CD)/2-g*v*FC+(1-e*v)*CE;
	CEF = f*v*(CF+CE)/2-g*v*FA+(1-e*v)*CE;
	CFA = f*v*(CA+CF)/2-g*v*FD+(1-e*v)*CF;
	CFE = f*v*(CF+CE)/2-g*v*FA+(1-e*v)*CF;
	CAG = (1-v)*CA+v*EF;
	CBG = (1-v)*CB+v*AC;
	CCG = (1-v)*CC+v*EH;
	CDG = (1-v)*CD+v*BD;
	CEG = (1-v)*CE+v*EE;
	CFG = (1-v)*CF+v*DA;
	CAH = f*(CB+CF)/4-g*(FD+FE)/2+e*CA/8;
	CBH = f*(CA+CC)/4-g*(FE+FK)/2+e*CB/8;
	CCH = f*(CB+CD)/4-g*(FK+FL)/2+e*CC/8;
	CDH = f*(CC+CE)/4-g*(FC+FL)/2+e*CD/8;
	CEH = f*(CD+CF)/4-g*(FA+FC)/2+e*CE/8;
	CFH = f*(CE+CA)/4-g*(FA+FD)/2+e*CF/8;

	DAB = f*v*(DA+DB)/2-g*v*FD+(1-e*v)*DA;
	DAF = f*v*(DA+DF)/2-g*v*FA+(1-e*v)*DA;
	DBA = f*v*(DA+DB)/2-g*v*FD+(1-e*v)*DB;
	DBC = f*v*(DB+DC)/2-g*v*FF+(1-e*v)*DB;
	DCB = f*v*(DB+DC)/2-g*v*FF+(1-e*v)*DC;
	DCD = f*v*(DC+DD)/2-g*v*FI+(1-e*v)*DC;
	DDC = f*v*(DC+DD)/2-g*v*FI+(1-e*v)*DD;
	DDE = f*v*(DD+DE)/2-g*v*FH+(1-e*v)*DD;
	DED = f*v*(DD+DE)/2-g*v*FH+(1-e*v)*DE;
	DEF = f*v*(DE+DF)/2-g*v*FB+(1-e*v)*DE;
	DFA = f*v*(DF+DA)/2-g*v*FA+(1-e*v)*DF;
	DFE = f*v*(DE+DF)/2-g*v*FB+(1-e*v)*DF;
	DAG = (1-v)*DA+v*CF;
	DBG = (1-v)*DB+v*EF;
	DCG = (1-v)*DC+v*AE;
	DDG = (1-v)*DD+v*EG;
	DEG = (1-v)*DE+v*BF;
	DFG = (1-v)*DF+v*EE;
	DAH = f*(DB+DF)/4-g*(FA+FD)/2+e*DA/8;
	DBH = f*(DA+DC)/4-g*(FD+FF)/2+e*DB/8;
	DCH = f*(DB+DD)/4-g*(FF+FI)/2+e*DC/8;
	DDH = f*(DC+DE)/4-g*(FH+FI)/2+e*DD/8;
	DEH = f*(DD+DF)/4-g*(FB+FH)/2+e*DE/8;
	DFH = f*(DE+DA)/4-g*(FA+FB)/2+e*DF/8;

	EEB = (1-v)*EE+v*BE;
	EEC = (1-v)*EE+v*CE;
	EED = (1-v)*EE+v*DF;
	EFC = (1-v)*EF+v*CA;
	EFA = (1-v)*EF+v*AD;
	EFD = (1-v)*EF+v*DB;
	EGA = (1-v)*EG+v*AF;
	EGB = (1-v)*EG+v*BA;
	EGD = (1-v)*EG+v*DD;
	EHB = (1-v)*EH+v*BC;
	EHA = (1-v)*EH+v*AB;
	EHC = (1-v)*EH+v*CC;
}

