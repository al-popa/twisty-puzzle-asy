void init()
{
	real a = 7/15;
	real b = 3/11;
	real c = 4/15;
	real d = 3/5;
	real e = 31/18;
	real f = 15/8;
	real g = 11/72;
	real u = 0.37; // Guess
	real v = 0.43; // Guess

	EA = a*(X+Y+Z);
	EB = a*(X-Y-Z);
	EC = a*(-X+Y-Z);
	ED = a*(-X-Y+Z);
	EE = d*(-X-Y-Z);
	EF = d*(-X+Y+Z);
	EG = d*(X-Y+Z);
	EH = d*(X+Y-Z);

	AA = EA+c*(2*X-Y-Z);
	AB = EA+c*(X+Y-2*Z);
	AC = EA+c*(-X+2*Y-Z);
	AD = EA+c*(-2*X+Y+Z);
	AE = EA+c*(-X-Y+2*Z);
	AF = EA+c*(X-2*Y+Z);

	BA = EB+c*(X-Y+2*Z);
	BB = EB+c*(2*X+Y+Z);
	BC = EB+c*(X+2*Y-Z);
	BD = EB+c*(-X+Y-2*Z);
	BE = EB+c*(-2*X-Y-Z);
	BF = EB+c*(-X-2*Y+Z);

	CA = EC+c*(-X+Y+2*Z);
	CB = EC+c*(X+2*Y+Z);
	CC = EC+c*(2*X+Y-Z);
	CD = EC+c*(X-Y-2*Z);
	CE = EC+c*(-X-2*Y-Z);
	CF = EC+c*(-2*X-Y+Z);

	DA = ED+c*(-2*X+Y-Z);
	DB = ED+c*(-X+2*Y+Z);
	DC = ED+c*(X+Y+2*Z);
	DD = ED+c*(2*X-Y+Z);
	DE = ED+c*(X-2*Y-Z);
	DF = ED+c*(-X-Y-2*Z);

	FA = b*(-3*X-Y-Z);
	FB = b*(-X-3*Y-Z);
	FC = b*(-X-Y-3*Z);
	FD = b*(-3*X+Y+Z);
	FE = b*(-X+3*Y+Z);
	FF = b*(-X+Y+3*Z);
	FG = b*(3*X-Y+Z);
	FH = b*(X-3*Y+Z);
	FI = b*(X-Y+3*Z);
	FJ = b*(3*X+Y-Z);
	FK = b*(X+3*Y-Z);
	FL = b*(X+Y-3*Z);

	GA = u*EH+(1-u)*FL;
	GB = u*BC+(1-u)*FL;
	GC = u*BD+(1-u)*FL;
	GD = u*CD+(1-u)*FL;
	GE = u*CC+(1-u)*FL;

	HA = u*EG+(1-u)*FH;
	HB = u*DD+(1-u)*FH;
	HC = u*DE+(1-u)*FH;
	HD = u*BF+(1-u)*FH;
	HE = u*BA+(1-u)*FH;

	IA = u*EF+(1-u)*FD;
	IB = u*CA+(1-u)*FD;
	IC = u*CF+(1-u)*FD;
	ID = u*DA+(1-u)*FD;
	IE = u*DB+(1-u)*FD;

	JA = u*EE+(1-u)*FA;
	JB = u*DF+(1-u)*FA;
	JC = u*DA+(1-u)*FA;
	JD = u*CF+(1-u)*FA;
	JE = u*CE+(1-u)*FA;

	KA = u*EH+(1-u)*FK;
	KB = u*CC+(1-u)*FK;
	KC = u*CB+(1-u)*FK;
	KD = u*AC+(1-u)*FK;
	KE = u*AB+(1-u)*FK;

	LA = u*EG+(1-u)*FI;
	LB = u*AF+(1-u)*FI;
	LC = u*AE+(1-u)*FI;
	LD = u*DC+(1-u)*FI;
	LE = u*DD+(1-u)*FI;

