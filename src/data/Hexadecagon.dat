	AAF = f*v*(AA+AF)/2-g*v*FG+(1-e*v)*AA;
	ABA = f*v*(AB+AA)/2-g*v*FJ+(1-e*v)*AB;
	ABC = f*v*(AB+AC)/2-g*v*FK+(1-e*v)*AB;
	ACB = f*v*(AB+AC)/2-g*v*FK+(1-e*v)*AC;
	ACD = f*v*(AD+AC)/2-g*v*FE+(1-e*v)*AC;
	ADC = f*v*(AD+AC)/2-g*v*FE+(1-e*v)*AD;
	ADE = f*v*(AE+AD)/2-g*v*FF+(1-e*v)*AD;
	AED = f*v*(AE+AD)/2-g*v*FF+(1-e*v)*AE;
	AEF = f*v*(AF+AE)/2-g*v*FI+(1-e*v)*AE;
	AFA = f*v*(AA+AF)/2-g*v*FG+(1-e*v)*AF;
	AFE = f*v*(AF+AE)/2-g*v*FI+(1-e*v)*AF;
	AAG = (1-v)*AA+v*BB;
	ABG = (1-v)*AB+v*EH;
	ACG = (1-v)*AC+v*CB;
	ADG = (1-v)*AD+v*EF;
	AEG = (1-v)*AE+v*DC;
	AFG = (1-v)*AF+v*EG;
	AAH = f*(AB+AF)/4-g*(FG+FJ)/2+e*AA/8;
	ABH = f*(AA+AC)/4-g*(FJ+FK)/2+e*AB/8;
	ACH = f*(AB+AD)/4-g*(FE+FK)/2+e*AC/8;
	ADH = f*(AC+AE)/4-g*(FE+FF)/2+e*AD/8;
	AEH = f*(AD+AF)/4-g*(FF+FI)/2+e*AE/8;
	AFH = f*(AA+AE)/4-g*(FG+FI)/2+e*AF/8;

	BAB = f*v*(BA+BB)/2-g*v*FG+(1-e*v)*BA;
	BAF = f*v*(BA+BF)/2-g*v*FH+(1-e*v)*BA;
	BBA = f*v*(BA+BB)/2-g*v*FG+(1-e*v)*BB;
	BBC = f*v*(BB+BC)/2-g*v*FJ+(1-e*v)*BB;
	BCB = f*v*(BB+BC)/2-g*v*FJ+(1-e*v)*BC;
	BCD = f*v*(BC+BD)/2-g*v*FL+(1-e*v)*BC;
	BDC = f*v*(BC+BD)/2-g*v*FL+(1-e*v)*BD;
	BDE = f*v*(BD+BE)/2-g*v*FC+(1-e*v)*BD;
	BED = f*v*(BD+BE)/2-g*v*FC+(1-e*v)*BE;
	BEF = f*v*(BE+BF)/2-g*v*FB+(1-e*v)*BE;
	BFA = f*v*(BF+BA)/2-g*v*FH+(1-e*v)*BF;
	BFE = f*v*(BE+BF)/2-g*v*FB+(1-e*v)*BF;
	BAG = (1-v)*BA+v*EG;
	BBG = (1-v)*BB+v*AA;
	BCG = (1-v)*BC+v*EH;
	BDG = (1-v)*BD+v*CD;
	BEG = (1-v)*BE+v*EE;
