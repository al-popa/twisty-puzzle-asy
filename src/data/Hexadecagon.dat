	NB = u*AB+(1-u)*FJ;
	NC = u*AA+(1-u)*FJ;
	ND = u*BB+(1-u)*FJ;
	NE = u*BC+(1-u)*FJ;

	OA = u*EF+(1-u)*FF;
	OB = u*DB+(1-u)*FF;
	OC = u*DC+(1-u)*FF;
	OD = u*AE+(1-u)*FF;
	OE = u*AD+(1-u)*FF;

	PA = u*EE+(1-u)*FC;
	PB = u*CE+(1-u)*FC;
	PC = u*CD+(1-u)*FC;
	PD = u*BD+(1-u)*FC;
	PE = u*BE+(1-u)*FC;

	QA = u*EG+(1-u)*FG;
	QB = u*BA+(1-u)*FG;
	QC = u*BB+(1-u)*FG;
	QD = u*AA+(1-u)*FG;
	QE = u*AF+(1-u)*FG;

