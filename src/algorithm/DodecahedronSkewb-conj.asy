makeChange(AQ--BJQ--CJ--BCQ, AQ--BEQ--CE--BJQ, 0.55*(BJQ+AQ));
makeChange(AM--BEM--CE--BIM, AM--BIM--CI--BAM, 0.55*(BIM+AM));
makeChange(AS--BJS--CJ--BDS, AS--BGS--CG--BJS, 0.55*(BJS+AS));
makeChange(AO--BGO--CG--BIO, AO--BIO--CI--BBO, 0.55*(BIO+AO));
figureLine();
DodecahedronSkewb();
makeMove(AS/3, 120, 1.0);
label("$L$", 1.15*AE);
figureNext();
DodecahedronSkewb();
makeMove(AO/3, -120, 1.0);
label("$R^{-1}$", 1.15*AE);
figureNext();
DodecahedronSkewb();
makeMove(AS/3, -120, 1.0);
label("$L^{-1}$", 1.15*AE);
figureNext();
DodecahedronSkewb();
makeMove(AO/3, 120, 1.0);
label("$R$", 1.15*AE);
figureEnd();
