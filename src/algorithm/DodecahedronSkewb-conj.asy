include "algorithm.asyhdr";
include "../data/DodecahedronSkewb.dat";

setView(0, -90, 0);

figureBegin();
DodecahedronSkewb();
highline(AM--BEM--CE--BIM);
highline(AQ--BEQ--CE--BJQ);
highline(AI--BIM--CE--BJQ--AJ);
highline(AJ--BJQ--CJ--BJS);
highline(AQ--BJQ--CJ--BCQ);
highline(AS--BJS--CJ--BDS);
highline(AC--BCQ--CJ--BDS--AD);
highline(AI--BIM--CI--BIO);
highline(AM--BIM--CI--BAM);
highline(AO--BIO--CI--BBO);
highline(AA--BAM--CI--BBO--AB);
highline(AG--BGO--CG--BGS);
highline(AO--BGO--CG--BIO);
highline(AS--BGS--CG--BJS);
highline(AI--BIO--CG--BJS--AJ);
makeChange(AJ--BJQ--CJ--BJS, AG--BGO--CG--BGS, 0.55*(BJS+AJ));
makeChange(AG--BGO--CG--BGS, AI--BIM--CI--BIO, 0.55*(BIO+AI));
