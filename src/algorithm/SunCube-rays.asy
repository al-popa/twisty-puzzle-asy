include "algorithm.asyhdr";
include "../data/SunCube.dat";

setView(45, -35.2, 0);

figureBegin();
SunCube();
highline(BAE--CACE--GC--DAECG);
highline(BGC--CGCE--GG--DCGAE);
makeChange(BAE--CACE--GC--DAECG, BBD--CBDF--HB--DBDFH, 1.05*(X-Z)+0.2*Y);
makeChange(BBD--CBDF--HB--DBDFH, BGC--CGCE--GG--DCGAE, 1.05*(X-Z)+0.05*Y);
makeChange(BGC--CGCE--GG--DCGAE, BAE--CACE--GC--DAECG, 1.15*Z);
figureNext();
setViewAbsolute(45, 35.2, 0);
SunCube();
highline(BBD--CBDF--HB--DBDFH);
makeChange(BAE--CACE--GC--DAECG, BBD--CBDF--HB--DBDFH, 1.05*(X-Z)+0.2*Y);
makeChange(BBD--CBDF--HB--DBDFH, BGC--CGCE--GG--DCGAE, 1.05*(X-Z)+0.05*Y);
setViewAbsolute(45, -35.2, 0);
figureLine();
SunCube();
makeMove(0.65*Z, -45, 1.5);
makeMove(0.75*Z, 45, 1.5);
makeMove(-0.65*Y, 180, 1.5);
makeMove(-0.75*Y, -180, 1.5);
label("$G^2$", 1.7*Z);
figureNext();
SunCube();
makeMove(-0.7*Y, 90, 1.5);
label("$L$", 1.7*Z);
figureNext();
SunCube();
makeMove(0.7*Z, 90, 1.5);
label("$U$", 1.7*Z);
figureLine();
SunCube();
makeMove(-0.7*Y, -90, 1.5);
label("$L^{-1}$", 1.7*Z);
figureNext();
SunCube();
makeMove(0.65*Z, 45, 1.5);
makeMove(0.75*Z, -45, 1.5);
makeMove(-0.65*Y, -180, 1.5);
makeMove(-0.75*Y, 180, 1.5);
label("$G^{-2}$", 1.7*Z);
figureEnd();
