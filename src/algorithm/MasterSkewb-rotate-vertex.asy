include "algorithm.asyhdr";
include "../data/MasterSkewb.dat";

setView(135, -35.2, 0);

figureBegin();
MasterSkewb();
highline(AA--BAB--BAC);
highline(AA--BAB--BAE);
highline(AA--BAC--BAE);
highline(AG--BGC--BGE);
makeChange(AA--BAC--BAE, AA--BAB--BAE, (7*AA+AE)/7);
makeChange(AA--BAB--BAC, AA--BAC--BAE, (7*AA+AC)/7);
makeChange(AA--BAB--BAE, AA--BAB--BAC, (7*AA+AB)/7);
makeChange(AG--BGC--BGE, AG--BGE--BGH, (7*AG+AE)/7);
makeChange(AG--BGC--BGH, AG--BGC--BGE, (7*AG+AC)/7);
figureLine();
MasterSkewb();
makeMove(0.45*AC, 120, 1.35);
makeMove(0.55*AE, 120, 1.25);
makeMove(0.55*AC, -120, 1.25);
makeMove(0.45*AE, -120, 1.35);
label("$F^3$", 1.7*Z);
figureNext();
MasterSkewb();
makeMove(0.5*AG, 120, 1.25);
label("$K$", 1.7*Z);
figureNext();
MasterSkewb();
makeMove(0.45*AC, -120, 1.35);
makeMove(0.55*AE, -120, 1.25);
makeMove(0.55*AC, 120, 1.25);
makeMove(0.45*AE, 120, 1.35);
label("$F^{-3}$", 1.7*Z);
figureNext();
MasterSkewb();
makeMove(0.5*AG, -120, 1.25);
label("$K^{-1}$", 1.7*Z);
figureEnd();
