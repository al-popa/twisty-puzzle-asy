include "algorithm.asyhdr";
include "../data/SunCube.dat";

setView(45, -35.2, 0);

figureBegin();
SunCube();
highline(DAECG--GC--GD);
highline(CACE--GB--GC);
highline(DACEG--GA--GB);
highline(CCAG--GH--GA);
highline(DCGAE--GG--GH);
makeChange(DBDFH--HA--HB, DAECG--GC--GD, 1.05*(X-Z)+0.15*Y);
makeChange(DAECG--GC--GD, DCGAE--GG--GH, 1.15*Z);
makeChange(DCGAE--GG--GH, DBDFH--HA--HB, 1.05*(X-Z)-0.15*Y);
makeChange(DACEG--GA--GB, CCAG--GH--GA, 1.1*GA);
makeChange(CCAG--GH--GA, CACE--GB--GC, 0.6*(GA+GB)-0.1*X);
makeChange(CACE--GB--GC, DACEG--GA--GB, 1.1*GB);
figureNext();
setViewAbsolute(45, 35.2, 0);
SunCube();
highline(DBDFH--HA--HB);
makeChange(DBDFH--HA--HB, DAECG--GC--GD, 1.05*(X-Z)+0.15*Y);
makeChange(DCGAE--GG--GH, DBDFH--HA--HB, 1.05*(X-Z)-0.15*Y);
setViewAbsolute(45, -35.2, 0);
figureLine();
SunCube();
makeMove(0.65*Z, -45, 1.5);
makeMove(0.75*Z, 45, 1.5);
makeMove(-0.65*Y, 180, 1.5);
makeMove(-0.75*Y, -180, 1.5);
label("$G^2$", 1.7*Z);
figureNext();
SunCube();
makeMove(0.7*Z, 90, 1.5);
label("$U$", 1.7*Z);
figureNext();
SunCube();
makeMove(0.65*Z, 45, 1.5);
makeMove(0.75*Z, -45, 1.5);
makeMove(-0.65*Y, -180, 1.5);
makeMove(-0.75*Y, 180, 1.5);
label("$G^{-2}$", 1.7*Z);
figureNext();
SunCube();
makeMove(0.7*Z, -90, 1.5);
label("$U^{-1}$", 1.7*Z);
figureEnd();
