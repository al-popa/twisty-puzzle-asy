include "algorithm.asyhdr";
include "../data/CubeSkewb.dat";

setView(135, -35.2, 0);

figureBegin();
CubeSkewb_1();
highline(CB--BAB--BAE);
makeChange(CD--BBD--BBF, CB--BAB--BAE, X/3+Y-Z);
makeChange(CB--BAB--BAE, CF--BEF--BEG, Z/3+Y-X);
makeChange(CF--BEF--BEG, CD--BBD--BBF, Y/3-X-Z);
figureNext();
setViewAbsolute(-135, 35.2, 0);
CubeSkewb_1();
highline(CB--BAB--BAE);
highline(CF--BEF--BEG);
highline(CD--BBD--BBF);
makeChange(CD--BBD--BBF, CB--BAB--BAE, X/3+Y-Z);
makeChange(CB--BAB--BAE, CF--BEF--BEG, Z/3+Y-X);
makeChange(CF--BEF--BEG, CD--BBD--BBF, Y/3-X-Z);
setViewAbsolute(135, -35.2, 0);
figureLine();
CubeSkewb_1();
makeMove(0.45*X, 90, 1.5);
makeMove(0.55*X, -90, 1.5);
makeMove(0.45*Y, -90, 1.5);
makeMove(0.55*Y, 90, 1.5);
label("$F$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove((-X-Y-Z)/3, -120, 1.7);
label("$K^{-1}$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove(0.45*X, -90, 1.5);
makeMove(0.55*X, 90, 1.5);
makeMove(0.45*Y, 90, 1.5);
makeMove(0.55*Y, -90, 1.5);
label("$F^{-1}$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove((-X-Y-Z)/3, 120, 1.7);
label("$K$", 1.7*Z);
figureLine();
CubeSkewb_1();
makeMove(0.45*X, 90, 1.5);
makeMove(0.55*X, -90, 1.5);
makeMove(0.45*Y, -90, 1.5);
makeMove(0.55*Y, 90, 1.5);
label("$F$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove((-X-Y-Z)/3, 120, 1.7);
label("$K$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove(0.45*X, -90, 1.5);
makeMove(0.55*X, 90, 1.5);
makeMove(0.45*Y, 90, 1.5);
makeMove(0.55*Y, -90, 1.5);
label("$F^{-1}$", 1.7*Z);
figureNext();
CubeSkewb_1();
makeMove((-X-Y-Z)/3, -120, 1.7);
label("$K^{-1}$", 1.7*Z);
figureEnd();
